@model GoodReceiveDetailModelList
    <div class="form-group row">
        <label class="col-sm-2 col-form-label">Location</label>
        <div class="col-sm-10">
            <select id="locationInput" class="form-control form-select">
                @foreach (var location in Model.Locations)
                {
                    <option value="@location.LocCde">@location.LocCde</option>
                }
            </select>
        </div>
    </div>
     <div class="form-group row">
        <label class="col-sm-2 col-form-label">Item ID</label>
        <div class="col-sm-10">
            <select id="ItemIDInput" class="form-control form-select">
                @foreach (var stock in Model.Stocks)
                {
                <option value="@stock.ItemId">@stock.ItemId</option>
                }
            </select>
        </div>
    </div>
